        <OpenJdkResource Include="$(OpenJdkDir)\jdk\src\share\demo\nio\zipfs\src\META-INF\services\java.nio.file.spi.FileSystemProvider" ResourcePath="META-INF/services/java.nio.file.spi.FileSystemProvider" />
        <OpenJdkResource Include="$(OpenJdkDir)\jdk\src\share\classes\sun\net\spi\nameservice\dns\META-INF\services\sun.net.spi.nameservice.NameServiceDescriptor" ResourcePath="META-INF/services/sun.net.spi.nameservice.NameServiceDescriptor" />
