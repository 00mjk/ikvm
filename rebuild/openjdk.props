<Project>
    <PropertyGroup>
        <OpenJdkDir>$(MSBuildThisFileDirectory)..\openjdk8</OpenJdkDir>
        <OpenJdkImplementationVersion>1.8.0</OpenJdkImplementationVersion>
        <OpenJdkSpecificationVersion>1.8</OpenJdkSpecificationVersion>
        <OpenJdkFullVersion>1.8.0_342-b02</OpenJdkFullVersion>
        <OpenJdkVersion>OpenJDK 8u342 b02</OpenJdkVersion>
    </PropertyGroup>
    
    <ItemDefinitionGroup>
        <OpenJdkSource>
            <SourcePath>%(Identity)</SourcePath>
        </OpenJdkSource>
    </ItemDefinitionGroup>
    
    <ItemGroup>
        <!-- jdk -->
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\share\classes\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\share\demo\nio\zipfs\src\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />

        <!-- pull in various Unix support from the Solaris source -->
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\solaris\classes\java\io\UnixFileSystem.java" PackagePath="java/io/UnixFileSystem" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\solaris\classes\java\util\prefs\FileSystemPreferences.java" PackagePath="java/util/prefs/FileSystemPreferences" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\solaris\classes\java\util\prefs\FileSystemPreferencesFactory.java" PackagePath="java/util/prefs/FileSystemPreferencesFactory" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\solaris\classes\sun\awt\windows\ThemeReader.java" PackagePath="sun/awt/windows/ThemeReader" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\solaris\classes\sun\net\www\protocol\http\ntlm\NTLMAuthentication.java" PackagePath="sun/net/www/protocol/http/ntlm/NTLMAuthentication" />
        
        <!-- pull in various Windows support from the Windows source -->
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\java\io\WinNTFileSystem.java" PackagePath="java/io/WinNTFileSystem" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\java\lang\ProcessEnvironment.java" PackagePath="java/lang/ProcessEnvironment" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\java\lang\Terminator.java" PackagePath="java/lang/Terminator" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\java\net\DefaultInterface.java" PackagePath="java/net/DefaultInterface" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\java\util\prefs\WindowsPreferences.java" PackagePath="java/util/prefs/WindowsPreferences" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\java\util\prefs\WindowsPreferencesFactory.java" PackagePath="java/util/prefs/WindowsPreferencesFactory" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\awt\Win32ColorModel24.java" PackagePath="sun/awt/Win32ColorModel24" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\management\FileSystemImpl.java" PackagePath="sun/management/FileSystemImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\management\OperatingSystemImpl.java" PackagePath="sun/management/OperatingSystemImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\net\NetHooks.java" PackagePath="sun/net/NetHooks" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\net\PortConfig.java" PackagePath="sun/net/PortConfig" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\DefaultAsynchronousChannelProvider.java" PackagePath="sun/nio/ch/DefaultAsynchronousChannelProvider" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\NativeThread.java" PackagePath="sun/nio/ch/NativeThread" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\PipeImpl.java" PackagePath="sun/nio/ch/PipeImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\sctp\SctpChannelImpl.java" PackagePath="sun/nio/ch/sctp/SctpChannelImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\sctp\SctpMultiChannelImpl.java" PackagePath="sun/nio/ch/sctp/SctpMultiChannelImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\sctp\SctpServerChannelImpl.java" PackagePath="sun/nio/ch/sctp/SctpServerChannelImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\SinkChannelImpl.java" PackagePath="sun/nio/ch/SinkChannelImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\SourceChannelImpl.java" PackagePath="sun/nio/ch/SourceChannelImpl" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\ch\WindowsAsynchronousChannelProvider.java" PackagePath="sun/nio/ch/WindowsAsynchronousChannelProvider" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\fs\WindowsPathParser.java" PackagePath="sun/nio/fs/WindowsPathParser" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\nio\fs\WindowsPathType.java" PackagePath="sun/nio/fs/WindowsPathType" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\print\Win32MediaTray.java" PackagePath="sun/print/Win32MediaTray" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\security\krb5\internal\tools\Kinit.java" PackagePath="sun/security/krb5/internal/tools/Kinit" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\security\krb5\internal\tools\KinitOptions.java" PackagePath="sun/security/krb5/internal/tools/KinitOptions" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\security\krb5\internal\tools\Klist.java" PackagePath="sun/security/krb5/internal/tools/Klist" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\security\krb5\internal\tools\Ktab.java" PackagePath="sun/security/krb5/internal/tools/Ktab" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\security\provider\NativePRNG.java" PackagePath="sun/security/provider/NativePRNG" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\security\provider\NativeSeedGenerator.java" PackagePath="sun/security/provider/NativeSeedGenerator" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\windows\classes\sun\util\locale\provider\HostLocaleProviderAdapterImpl.java" PackagePath="sun/util/locale/provider/HostLocaleProviderAdapterImpl" />
        
        <!-- jaxp -->
        <OpenJdkSource Include="$(OpenJdkDir)\jaxp\src\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        
        <!-- jaxws -->
        <OpenJdkSource Include="$(OpenJdkDir)\jaxws\src\share\jaf_classes\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        <OpenJdkSource Include="$(OpenJdkDir)\jaxws\src\share\jaxws_classes\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        
        <!-- corba -->
        <OpenJdkSource Include="$(OpenJdkDir)\corba\src\share\classes\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        
        <!-- langtools -->
        <OpenJdkSource Include="$(OpenJdkDir)\langtools\src\share\classes\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        
        <!-- nashorn (removed in later JDKs) -->
        <OpenJdkSource Include="$(OpenJdkDir)\nashorn\buildtools\nasgen\src\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        <OpenJdkSource Include="$(OpenJdkDir)\nashorn\src\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        
        <!-- source files were generated by the build -->
        <OpenJdkSource Include="$(OpenJdkDir)\build\linux-x86_64-normal-server-release\jdk\gensrc\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        <OpenJdkSource Include="$(OpenJdkDir)\build\linux-x86_64-normal-server-release\jdk\impsrc\com\sun\corba\se\impl\logging\**\*.java" PackagePath="com/sub/corba/se/impl/logging/$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        <OpenJdkSource Include="$(OpenJdkDir)\build\linux-x86_64-normal-server-release\langtools\gensrc\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        <OpenJdkSource Include="$(OpenJdkDir)\build\linux-x86_64-normal-server-release\corba\gensrc\**\*.java" PackagePath="$([System.String]::Copy('%(RecursiveDir)%(Filename)').Replace('\', '/'))" />
        
        <!-- X11 does not build correctly -->
        <OpenJdkSource Remove="$(OpenJdkDir)\build\linux-x86_64-normal-server-release\jdk\gensrc\sun\awt\X11\**\*.java" />
    
        <!-- GTK look and feel does not build correctly -->
        <OpenJdkSource Remove="$(OpenJdkDir)\jdk\src\share\classes\com\sun\java\swing\plaf\gtk\**\*.java" />
        
        <!-- SNMP does not build correctly -->
        <OpenJdkSource Remove="$(OpenJdkDir)\jdk\src\share\classes\com\sun\jmx\snmp\**\*.java" />
        <OpenJdkSource Remove="$(OpenJdkDir)\jdk\src\share\classes\sun\management\snmp\**\*.java" />

        <!-- Sound stuff is overridden by icedtea/rt -->
        <OpenJdkSource Remove="$(OpenJdkDir)\jdk\src\share\classes\com\sun\media\sound\*.java" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\share\classes\com\sun\media\sound\MidiDeviceReceiverEnvelope.java" PackagePath="com/sun/media/sound/MidiDeviceReceiverEnvelope" />
        <OpenJdkSource Include="$(OpenJdkDir)\jdk\src\share\classes\com\sun\media\sound\MidiDeviceTransmitterEnvelope.java" PackagePath="com/sun/media/sound/MidiDeviceTransmitterEnvelope" />

        <!-- Ductus Rendering Engine -->
        <OpenJdkSource Remove="$(OpenJdkDir)\jdk\src\share\classes\sun\dc\**\*.java" />

        <!-- Wasn't included by Jeoren, unknown -->
        <OpenJdkSource Remove="$(OpenJdkDir)\jdk\src\share\classes\sun\font\SunLayoutEngine.java" />

    </ItemGroup>

    <ItemGroup>
        <OpenJdkResource Include="$(OpenJdkDir)\build\linux-x86_64-normal-server-release\jdk\classes\java\lang\uniName.dat" ResourcePath="java/lang/uniName.dat" />
