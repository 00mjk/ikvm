<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageId>ikvmstub</PackageId>
        <OutputType>Exe</OutputType>
        <TargetFrameworks>net461;netcoreapp3.1</TargetFrameworks>
        <RuntimeIdentifiers>win7-x64;linux-x64</RuntimeIdentifiers>
        <AssemblyTitle>IKVM.NET Assembly Exporter</AssemblyTitle>
        <Description>IKVM.NET Assembly Exporter</Description>
        <RootNamespace>ikvmstub</RootNamespace>
        <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
        <PreserveCompilationContext>true</PreserveCompilationContext>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\IKVM.Tool.Exporter\IKVM.Tool.Exporter.csproj" />
    </ItemGroup>

</Project>
