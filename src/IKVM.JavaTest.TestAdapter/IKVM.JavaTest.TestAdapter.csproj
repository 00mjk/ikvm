<Project>
    <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props"/>
    <Import Project="$(MSBuildThisFileDirectory)..\IKVM-pkg\buildTransitive\netstandard2.0\IKVM.props" />
    <Import Project="$(MSBuildThisFileDirectory)..\IKVM.MSBuild\buildTransitive\netstandard2.0\IKVM.MSBuild.props" />
    <Import Project="$(MSBuildThisFileDirectory)..\..\jtreg.props" />

    <PropertyGroup>
        <TargetFrameworks>net461;netcoreapp3.1</TargetFrameworks>
    </PropertyGroup>

    <ItemGroup>
        <InternalsVisibleTo Include="IKVM.JavaTest.TestAdapter.Tests" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\IKVM.Java\IKVM.Java.msbuildproj">
            <IsIkvmBaseAssembly>true</IsIkvmBaseAssembly>
        </ProjectReference>
        <ProjectReference Include="..\IKVM.Runtime\IKVM.Runtime.csproj">
            <IsIkvmRuntimeAssembly>true</IsIkvmRuntimeAssembly>
        </ProjectReference>
        <ProjectReference Include="..\IKVM.Runtime.JNI\IKVM.Runtime.JNI.csproj">
            <IsIkvmRuntimeJNIAssembly>true</IsIkvmRuntimeJNIAssembly>
        </ProjectReference>
        <ProjectReference Include="..\javac\javac.csproj">
            <PrivateAssets>All</PrivateAssets>
            <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
            <Private>False</Private>
            <IsJavaCompiler>true</IsJavaCompiler>
        </ProjectReference>
        <ProjectReference Include="..\ikvmc\ikvmc.csproj">
            <PrivateAssets>All</PrivateAssets>
            <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
            <Private>False</Private>
            <IsIkvmCompiler>true</IsIkvmCompiler>
        </ProjectReference>
        <ProjectReference Include="..\ikvmstub\ikvmstub.csproj">
            <PrivateAssets>All</PrivateAssets>
            <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
            <Private>False</Private>
            <IsIkvmExporter>true</IsIkvmExporter>
        </ProjectReference>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.TestPlatform.AdapterUtilities" Version="17.2.0" />
        <PackageReference Include="Microsoft.TestPlatform.ObjectModel" Version="17.2.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\IKVM.JavaTest\IKVM.JavaTest.msbuildproj" />
    </ItemGroup>

    <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
        <PublishProjectReference Include="..\IKVM.JavaTest.AgentServer\IKVM.JavaTest.AgentServer.csproj">
            <SetTargetFramework>TargetFramework=net461</SetTargetFramework>
            <PublishTargetPath>agent-server\win7-x64</PublishTargetPath>
            <PublishProperties>RuntimeIdentifier=win7-x64</PublishProperties>
        </PublishProjectReference>
    </ItemGroup>

    <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.1' ">
        <PublishProjectReference Include="..\IKVM.JavaTest.AgentServer\IKVM.JavaTest.AgentServer.csproj">
            <SetTargetFramework>TargetFramework=netcoreapp3.1</SetTargetFramework>
            <PublishTargetPath>agent-server\win7-x64</PublishTargetPath>
            <PublishProperties>RuntimeIdentifier=win7-x64</PublishProperties>
        </PublishProjectReference>
        <PublishProjectReference Include="..\IKVM.JavaTest.AgentServer\IKVM.JavaTest.AgentServer.csproj">
            <SetTargetFramework>TargetFramework=netcoreapp3.1</SetTargetFramework>
            <PublishTargetPath>agent-server\linux-x64</PublishTargetPath>
            <PublishProperties>RuntimeIdentifier=linux-x64</PublishProperties>
        </PublishProjectReference>
    </ItemGroup>

    <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
        <PublishProjectReference Include="..\java\java.csproj">
            <SetTargetFramework>TargetFramework=net461</SetTargetFramework>
            <PublishTargetPath>jdk\bin</PublishTargetPath>
            <PublishProperties></PublishProperties>
        </PublishProjectReference>
        <PublishProjectReference Include="..\java\java.csproj">
            <SetTargetFramework>TargetFramework=net461</SetTargetFramework>
            <PublishTargetPath>jdk\bin</PublishTargetPath>
            <PublishProperties></PublishProperties>
        </PublishProjectReference>
    </ItemGroup>

    <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.1' ">
        <PublishProjectReference Include="..\java\java.csproj">
            <SetTargetFramework>TargetFramework=netcoreapp3.1</SetTargetFramework>
            <PublishTargetPath>jdk\bin</PublishTargetPath>
            <PublishProperties></PublishProperties>
        </PublishProjectReference>
        <PublishProjectReference Include="..\javac\javac.csproj">
            <SetTargetFramework>TargetFramework=netcoreapp3.1</SetTargetFramework>
            <PublishTargetPath>jdk\bin</PublishTargetPath>
            <PublishProperties></PublishProperties>
        </PublishProjectReference>
    </ItemGroup>

    <ItemGroup>
        <IkvmReference Include="@(JTRegJar)" />
    </ItemGroup>

    <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets"/>
    <Import Project="$(MSBuildThisFileDirectory)..\IKVM-pkg\buildTransitive\netstandard2.0\IKVM.targets" />
    <Import Project="$(MSBuildThisFileDirectory)..\IKVM.MSBuild\buildTransitive\netstandard2.0\IKVM.MSBuild.targets" />
</Project>
